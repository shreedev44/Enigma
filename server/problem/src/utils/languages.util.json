{
  "javascript": {
    "extension": "js",
    "image": "node:alpine",
    "command": "node /app/{filename}"
  },
  "python": {
    "extension": "py",
    "image": "python:alpine",
    "command": "python3 /app/{filename}"
  },
  "java": {
    "extension": "java",
    "image": "openjdk:alpine",
    "compile": "javac /app/{filename}",
    "command": "java -cp /app {mainClass}"
  }
}
